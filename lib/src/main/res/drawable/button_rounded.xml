<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="16dp" />
            <stroke android:width="2dp" android:color="@color/colorButtonLoginPressed" />
            <solid android:color="@color/colorButtonLoginPressed" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="16dp" />
            <stroke android:width="1dp" android:color="@color/colorButtonLoginDefault" />
            <solid android:color="@color/colorButtonLoginDefault"/>
        </shape>
    </item>
</selector>